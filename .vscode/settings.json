{
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "quickfix.biome": true
  },
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "cSpell.words": [
    "Antd",
    "anticon",
    "builtins",
    "colorfy",
    "Faas",
    "faasjs",
    "onwarn",
    "querystringify",
    "RUNENV",
    "SECRETID",
    "SECRETKEY",
    "SESSIONTOKEN",
    "Tencentcloud",
    "tencentcloudapi",
    "tsup",
    "typedoc",
    "Useify",
    "vuepress",
    "zfben"
  ]
}
